{
    "name": "motion-vuejs",
    "version": "0.0.5",
    "description": "A simple sheet component built with Vue.js",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "dist/vue-motion.umd.cjs",
    "types": "dist/index.d.ts",
    "module": "dist/vue-motion.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/vue-motion.js",
            "require": "./dist/vue-motion.umd.cjs",
            "default": "./dist/vue-motion.umd.cjs"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build --mode lib && cp src/index.d.ts dist/index.d.ts",
        "preview": "vite preview"
    },
    "dependencies": {
        "vue": "^3.2.45",
        "motion": "^12.4.3"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "5.2.1",
        "sass": "1.85.0",
        "vite": "6.1.0"
    },
    "author": {
        "name": "Calum Chamberlain",
        "email": "calum@appoly.co.uk"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/CalumChamberlain/vue-motion/issues"
    },
    "homepage": "https://github.com/CalumChamberlain/vue-motion#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CalumChamberlain/vue-motion.git"
    },
    "keywords": [
        "vuejs",
        "motion",
        "animation"
    ]
}